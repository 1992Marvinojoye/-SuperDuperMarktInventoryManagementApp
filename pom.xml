<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId> SuperDuperMarkt.Inventory.Management.App</groupId>
	<artifactId> SuperDuperMarkt.Inventory.Management.App</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name> SuperDuperMarktInventoryManagementApp</name>
	<description>The SuperDuperMarktInventoryManagementApp ist a Console Application and a Maven based Java Project that can have optional an UI and this App simulates a Supermarket Inventory</description>


	<properties>
		<maven.compiler.source>3.8.1</maven.compiler.source>
		<maven.compiler.target>3.8.1</maven.compiler.target>
		<maven.compiler.release>11</maven.compiler.release>
	</properties>

	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>3.8.1</version>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>

	<dependencies>
		<!-- https://search.maven.org/artifact/com.weicoder/log4j/3.4.6/jar -->
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.12</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>com.weicoder</groupId>
			<artifactId>log4j</artifactId>
			<version>3.4.6</version>
		</dependency>
		<!-- https://mvnrepository.com/artifact/com.fasterxml.jackson.core/jackson-core -->
		<dependency>
			<groupId>com.fasterxml.jackson.core</groupId>
			<artifactId>jackson-core</artifactId>
			<version>2.12.3</version>
		</dependency>


	</dependencies>

</project>